#
# Settings for additional CMIP6 output
#

VERSION_ = $$Id: cmip6.config 1 2020-11-11 09:47:23Z m221078 $$

LAND_OUTPUT = none

[namelists]

  [[NAMELIST_atm]]

    [[[output_nml atm_dbg]]]
      .hide = true

    [[[output_nml atm_P1D_2d]]]
      filetype = 5
      output_filename = ${EXP_ID}_atm_P1D_2d
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1D
      filename_format = <output_filename>_<datetime2>
      file_interval = $${atm_file_interval}
      mode = 1
      operation = mean
      include_last = false
      output_grid = true
      m_levels = 1
      ml_varlist = ps, psl, rsdt, rsut, rlut, rsds, rlds, rsus, rlus, ts, clt, pr, prw, cllvi, clivi, hfls, hfss, evspsbl, tauu, tauv, sfcwind, uas, vas, tas
  #   ml_varlist = tas, pr, rlut
  #

    [[[output_nml lnd_fract]]]
      filetype = 5
      output_filename = ${EXP_ID}_lnd_fract
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1M
      filename_format = <output_filename>_<levtype_l>_<datetime2>
      output_grid = true
      file_interval = $${lnd_file_interval}
      include_last = false
      ml_varlist = group:jsb_tile_fractions

    [[[output_nml lnd_P1M]]]
      filetype = 5
      output_filename = ${EXP_ID}_lnd_P1M
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1M
      filename_format = <output_filename>_<datetime2>
      operation = mean
      file_interval = $${lnd_file_interval}
      include_last = false
      output_grid = true
      # Most carbon variables are missing, as C-cycle was swithed off in spinup runs
      # hydro_runoff_glac_box:  not aggregated in current version -> removed from list
      # carbon_GPP_yDayMean_box -> carbon_GPP_yDayMean_ta_box
      # carbon_NPP_yDayMean_box -> carbon_NPP_yDayMean_ta_box
      ml_varlist = assimi_gross_assimilation_box, assimi_dark_respiration_box, carbon_GPP_yDayMean_ta_box, carbon_NPP_act_yDayMean_ta_box, hydro_evapopot_box, hydro_evapopot_land, hydro_evapopot_lake, hydro_evapotrans_box, hydro_trans_box, hydro_fract_snow_box, hydro_w_snow_box, hydro_snowmelt_box, hydro_runoff_box, hydro_runoff_land, hydro_drainage_box, hydro_drainage_land, hydro_discharge_ocean_box, hydro_discharge_box, hydro_w_soil_sl_box, hydro_w_ice_sl_box, hydro_w_soil_column_box, pheno_lai_box, pheno_fract_fpc_box, rad_alb_vis_box, rad_alb_vis_land, rad_alb_vis_lake, rad_alb_vis_veg, rad_alb_nir_box, rad_alb_nir_land, rad_alb_nir_lake, rad_alb_nir_veg, rad_rad_srf_net_box, rad_rad_srf_net_land, rad_rad_srf_net_lake, rad_lw_srf_net_box, rad_lw_srf_net_land, rad_lw_srf_net_lake, rad_lw_srf_net_veg, rad_sw_srf_net_box, rad_sw_srf_net_land, rad_sw_srf_net_lake, rad_sw_srf_net_veg, rad_alb_vis_snow_box, rad_alb_vis_snow_land, rad_alb_vis_snow_lake, rad_fapar_box, rad_fapar_land, rad_fapar_veg, rad_par_box, seb_t_land, seb_t_lake, seb_latent_hflx_land,  seb_latent_hflx_lake, seb_sensible_hflx_land, seb_sensible_hflx_lake, seb_forc_hflx_box, seb_fract_lice_box, seb_qsat_star_box, sse_t_soil_box, sse_t_soil_land, hydro_w_snow_can_box, hydro_evapotrans_soil_box

    [[[output_nml lnd_P1D]]]
      filetype = 5
      output_filename = ${EXP_ID}_lnd_P1D
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1D
      filename_format = <output_filename>_<datetime2>
      operation = mean
      file_interval = $${lnd_file_interval}
      include_last = false
      output_grid = true
      ml_varlist = hydro_w_soil_sl_box, hydro_w_ice_sl_box, hydro_w_soil_column_box, hydro_runoff_box, hydro_drainage_box, hydro_fract_snow_box, hydro_w_snow_box, hydro_snowmelt_box, hydro_w_snow_can_box, pheno_fract_fpc_box, pheno_lai_box, sse_t_soil_land, hydro_evapotrans_soil_box


  [[NAMELIST_oce]]

    [[[output_nml oce_PT3H_inst]]]
      filetype = 5
      output_filename = ${EXP_ID}_oce_PT3H_inst
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = "PT3H"
      filename_format = <output_filename>_<datetime2>
      file_interval = $${oce_file_interval}
      mode = 1
      include_last = false
      output_grid = true
      m_levels         = 1
      ml_varlist       = to

    [[[output_nml oce_P1D_2d]]]
      filetype = 5
      output_filename = ${EXP_ID}_oce_P1D_2d
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1D
      filename_format = <output_filename>_<datetime2>
      operation = mean
      file_interval = $${oce_file_interval}
      mode = 1
      include_last = false
      output_grid = true
      m_levels = 1
      ml_varlist = to, so, mlotst, hi, hs, conc, ice_u, ice_v

    [[[output_nml oce_P1D_3d]]]
      filetype = 5
      output_filename = ${EXP_ID}_oce_P1D_3d
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1D
      filename_format = <output_filename>_<datetime2>
      operation = mean
      file_interval = $${oce_file_interval}
      mode = 1
      include_last = false
      output_grid = true
      m_levels = "1...37"
      ml_varlist = to

    [[[output_nml oce_P1D_max]]]
      filetype = 5
      output_filename = ${EXP_ID}_oce_P1D_max
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1D
      filename_format = <output_filename>_<datetime2>
      operation = max
      file_interval = $${oce_file_interval}
      mode = 1
      include_last = false
      output_grid = true
      ml_varlist = mlotst

    [[[output_nml oce_P1D_sqr]]]
      filetype = 5
      output_filename = ${EXP_ID}_oce_P1D_sqr
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1D
      filename_format = <output_filename>_<datetime2>
      operation = square
      file_interval = $${oce_file_interval}
      mode = 1
      include_last = false
      output_grid = true
      m_levels = 1
      ml_varlist = to, so


    [[[output_nml oce_P1M_min]]]
      filetype = 5
      output_filename = ${EXP_ID}_oce_P1M_min
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1M
      filename_format = <output_filename>_<datetime2>
      operation = min
      file_interval = $${oce_file_interval}
      mode = 1
      include_last = false
      output_grid = true
      ml_varlist = mlotst

    [[[output_nml oce_P1M_max]]]
      filetype = 5
      output_filename = ${EXP_ID}_oce_P1M_max
      output_start = $${initial_date}
      output_end = $${final_date}
      output_interval = P1M
      filename_format = <output_filename>_<datetime2>
      file_interval = $${oce_file_interval}
      mode = 1
      include_last = false
      output_grid = true
      ml_varlist = mlotst

  [[NAMELIST_lnd]]
    with_carbon = true

[output]
  atm_P1D_2d = true
  atm_dbg_ml = false
  lnd_P1D = true
  lnd_P1M = true
  oce_P1D_2d = true
  oce_P1D_3d = true
  oce_P1D_max = true
  oce_P1D_sqr = true
  oce_P1M_max = true
  oce_P1M_min = true
  oce_PT3H_inst = true
  [[fixed]]
    lnd_fract_ml = true
  [[pressure_level]]
    [[[atm_3d_pl]]]
      input_tag = atm_3d_ml
      variable_names = ta, ua, va, zg, hur, hus, wap
      pressure_levels = 100000, 92500, 85000, 70000, 60000, 50000, 40000, 30000, 25000, 20000, 17000, 15000, 13000, 11500, 10000, 9000, 8000, 7000, 5000, 3000, 2000, 1500, 1000, 700, 500, 300, 200, 150, 100, 70, 50, 40, 30, 20, 15, 10, 7, 5, 3

