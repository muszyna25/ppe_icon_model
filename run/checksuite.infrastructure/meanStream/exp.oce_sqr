  include_last     = .false.
  output_grid      = .TRUE.
  operation        = "square"
  filename_format  = "<output_filename>_<datetime2>"
 ml_varlist       = ${varnames}
/
&output_nml
  filetype         =  4 ! output format: 2=GRIB2, 4=NETCDFv2
  output_filename  = "${EXPNAME}_square3TS"
  filename_format  = "<output_filename>_<datetime2>"
  output_start     = "${start_date}"                  ! start in ISO-format
