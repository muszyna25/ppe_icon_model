VERSION_ = 

ATMO_TIME_STEP = PT20M # 72 time steps per day
OCEAN_TIME_STEP = PT60M # 24 time steps per day

INTERVAL = P10Y
OUTPUT_INTERVAL = P1Y

[jobs]
  [[run]]
    nodes = 20
    time_limit = 02:00:00
  [[run_start]]
     .extends = run
     init_ocean = fromRestart
     init_obgc = true
  [[post]]
    tasks = 8
    time_limit = 00:05:00
  [[mon]]
    tasks = 4
    time_limit = 00:05:00
