    [[[master_model_nml atmo]]]
      model_rank_group_size = 1
    [[[master_model_nml ocean]]]
      model_rank_group_size = 1

  [[coupling.xml]]
    .use_template = coupling_passthrough.xml
    include_slp = true

[files]
  
  [[coupler]]
    .base_dir = $MODEL_DIR/externals/yac/input
    .method = cp -fv
    coupling.xsd = .
  
  [[atmosphere]]
    [[[model]]]
      .base_dir = $MODEL_DIR
      [[[[data]]]]
        .sub_dir = data
        lsdata.nc = 
        ECHAM6_CldOptProps.nc = 
        rrtmgp-cloud-optics-coeffs-lw.nc = ECHAM6_CldOptProps_rrtmgp_lw.nc
        rrtmgp-cloud-optics-coeffs-sw.nc = ECHAM6_CldOptProps_rrtmgp_sw.nc
      [[[[rrtmgp]]]]
        .sub_dir = externals/rte-rrtmgp/rrtmgp/data
        coefficients_lw.nc = rrtmgp-data-lw-g128-210809.nc
        coefficients_sw.nc = rrtmgp-data-sw-g112-210809.nc
      [[[[run]]]]
        .method = cp -fv
        .sub_dir = run
        dict.txt = dict.iconam.mpim
    [[[independent]]]
      .sub_dir = independent
      bc_greenhouse_gases.nc = greenhouse_gases/greenhouse_$SCENARIO.nc
      [[[[solar_irradiance]]]]
        .sub_dir = solar_radiation/3.2
        bc_solar_irradiance_sw_b14.nc = swflux_14band_cmip6_1850-2299-v3.2.nc
    [[[mapped]]]
      hd_mask.nc = icon_grid_${ATMO_GRID_ID}_${ATMO_GRID_TYPE}_G.nc
      [[[[initial]]]]
        .sub_dir = initial_condition/r0002_fast
        ifs2icon.nc = ifs2icon_2020012000_${ATMO_GRID_TYPE}_G.nc
      [[[[ozone]]]]
        .offsets = 0
        .set label = %year, 2015:2014
        .sub_dir = ozone/r0002_fast
        bc_ozone.nc = bc_ozone_historical_$${label}.nc
      [[[[ocean_surface]]]]
        .offsets = 0
        .sub_dir = sst_and_seaice/r0002_fast
        bc_sic.nc = bc_sic_%year.nc
        bc_sst.nc = bc_sst_%year.nc
      [[[[aerosols]]]]
        .sub_dir = aerosol_kinne/r0002_fast
        bc_aeropt_kinne_lw_b16_coa.nc = .
        bc_aeropt_kinne_sw_b14_coa.nc = .
        bc_aeropt_kinne_sw_b14_fin_2020.nc = .
    [[[restart]]]
      multifile_restart_atm.mfr = ${EXP_ID}_restart_atm_$${start_stamp}Z.mfr
      restart_atm_DOM01.nc = 
  
  [[land]]
    [[[mapped]]]
      .sub_dir = $ATMO_GRID_ID-$OCEAN_GRID_ID/land/r0005_fast
      [[[[hd]]]]
        .sub_dir = ../r0006
        bc_land_hd.nc = hdpara_${atmo_grid}_bifurcated_final.nc
        ic_land_hd.nc = hdstart_from_dpp0057_v3_nogrid.nc
    [[[hydro]]]
      .hide = true
  
  [[ocean]]
    [[[grids]]]
      .sub_dir = $OCEAN_GRID_ID
    [[[restart]]]
      multifile_restart_ocean.mfr = ${EXP_ID}_restart_oce_$${start_stamp}Z.mfr
      restart_ocean_DOM01.nc = 

[jobs]
  [[run]]
    .trigger = run, intake -w -W # disable post-processing and monitoring for now
    nodes = 400
    threads_per_task = 4
    share_nodes = true
    ocean_tasks_per_node = 8
    time_limit = 02:00:00
  [[run_start]]
    [[[namelists]]]
      [[[[icon_master.namelist]]]]
        [[[[[jsb_control_nml]]]]]
          restart_jsbach = false
        [[[[[master_nml]]]]]
          lrestart = false
      [[[[NAMELIST_lnd]]]]
        [[[[[jsb_hd_nml]]]]]
          read_initial_reservoirs = true
      [[[[NAMELIST_oce]]]]
        [[[[[ocean_initialconditions_nml]]]]]
          initialize_fromrestart = true # do not initialize ocean
    [[[files]]]
      [[[[atmosphere]]]]
        [[[[[restart_data]]]]]
          .hide = true
        [[[[[restart]]]]]
          .hide = true
      [[[[ocean]]]]
        [[[[[restart_data]]]]]
          .hide = true
        [[[[[restart]]]]]
          .base_dir = $OCEAN_PARENT_DIR
          multifile_restart_ocean.mfr = ${OCEAN_PARENT_EXP_ID}_restart_oce_${OCEAN_PARENT_DATE}.nc
          restart_ocean_DOM01.nc = 
