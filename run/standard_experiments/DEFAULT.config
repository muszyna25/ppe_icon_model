OCEAN_INPUT_SUBDIR = ICON/oes/input/r0004
OBGC_INPUT_SUBDIR = $OCEAN_INPUT_SUBDIR
COUPLER_INPUT_SUBDIR = $INPUT_SUBDIR

ATMO_SUBDIR = 
LAND_SUBDIR = 
OCEAN_SUBDIR = 
OBGC_SUBDIR = 
COUPLER_SUBDIR = 

