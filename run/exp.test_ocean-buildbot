#!/bin/bash

check_accept_script()
{
  #set -x
  check_accepted_filename=$1
  accepted_script="true"
  
  case $check_accepted_filename in
    *.run)
       accepted_script="false"
       ;;
    *~)
       accepted_script="false"
       ;;
    *.status)
       accepted_script="false"
       ;;
   esac
   #--------------------------
   if [[ $accepted_script = "false" ]] ; then
    return 1
   fi

}
#---------------------------------------------------------------


buildbold_folder="checksuite.ocean_internal/buildbot"
buildbot_ocean_exps=`ls $buildbold_folder`

for exp in $buildbot_ocean_exps ; do

  check_accept_script $exp
  if [ $accepted_script = "false" ]; then
    continue
  fi
  
  cd $thisdir
  number_of_required_files=0
  number_of_linked_files=0
  
  echo $exp
  export EXPNAME=${exp#*.}  
  . ${thisdir}/${buildbold_folder}/$exp
  . ${thisdir}/exec.iconrun
  
done

exit
