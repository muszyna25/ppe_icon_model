         unknown)
           # precise, unknown compiler
           icon_optim_error="unable to set '$enable_optimization' \
optimizations for unknown Fortran compiler" ;; #(
         intel)
           # precise, Intel compiler (example)
           icon_optim_FCFLAGS='-O2 -fp-model precise'
