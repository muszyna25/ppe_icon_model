# This Makefile sets up the building environment and calls Makefile.main

SHELL= @SHELL@
BUILD_ENV= @BUILD_ENV@
DELEGATED_RULES = all depend mostlyclean clean maintainer-clean install srclist

.SUFFIXES:
.PHONY: $(DELEGATED_RULES) distclean

$(DELEGATED_RULES):
	@$(BUILD_ENV) $(MAKE) -f Makefile.main $(MAKECMDGOALS)

distclean: clean
	rm -f Makefile

